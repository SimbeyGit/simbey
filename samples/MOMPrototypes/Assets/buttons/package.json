[
	{
		package: "buildsif",
		name: "buttons.sif",
		trim: true,
		files: ["*.png"]
	},
	{
		package: "json",
		name: "animations.json",
		data:
		{
			type: "animation",
			animation: "buttons",
			images:
			[
				{ layer: 0 },	// button74x21Disabled
