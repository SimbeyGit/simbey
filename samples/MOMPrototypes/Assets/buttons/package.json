				{ layer: 2 },	// button74x21Pressed
			],
			animations:
			[
				// button74x21Disabled (animation 0)
				{
					frames:
					[
						{ image: 0, ticks: 1, xoffset: 0, yoffset: 0 }
					]
				},
				// button74x21Normal (animation 1)
				{
					frames:
					[
						{ image: 1, ticks: 1, xoffset: 0, yoffset: 0 }
					]
				},
				// button74x21Pressed (animation 2)
				{
					frames:
					[
						{ image: 2, ticks: 1, xoffset: 0, yoffset: 0 }
					]
				}
			]
		}
	}
]